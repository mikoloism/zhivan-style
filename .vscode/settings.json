{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true,
        ".eslintrc.json": true,
        "**/*.d.ts": true,
        "**/.prettierrc": true,
        ".gitignore": true,
        "package-lock.json": true,
        "node_modules": true,
