{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true,
        ".eslintrc.json": true,
        "**/*.d.ts": true,
        "**/.prettierrc": true
    },
    "explorerExclude.backup": {},
    "conventionalCommits.scopes": [
        "component",
        "style",
        "page",
        "story",
        "type",
        "assets",
        "hook",
        "util",
        "config",
        "package",
        "build",
        "release",
        "webpack",
        "todo",
        "readme",
        "changelog",
        "license",
        "credits",
        "guidelines",
        "contributing",
        "merge",
        "pull-request",
        "issues",
        "tags"
    ],
    "typescript.tsdk": "node_modules/typescript/lib",
    "cSpell.words": ["asadpour", "Bakh", "Yekan", "اسدپور"]
}
